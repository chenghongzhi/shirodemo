<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.mapper.TeacherStudentMapper">
    <insert id="insertForEach" parameterType="com.example.model.TeacherStudent">
        insert into teacher_student(teacher_id, student_id)
        values
        <foreach collection="lists" item="list" index="index" separator=",">
            (#{list.teacherId},
            #{list.studentId})
        </foreach>
    </insert>
</mapper>